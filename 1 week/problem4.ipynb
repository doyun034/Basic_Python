   "source": [
    "sec=int(input(\"초단위 시간을 입력하시오\"))  # sec 변수 선언\n",
    "hour = sec //3600  # hour 변수를 sec // 3600으로 지정\n",
    "min = (sec%3600)//60 # min 변수를 (sec%3600)//60으로 지정 \n",
    "re = sec % 60 # re 변수를 sec % 60으로 지정\n",
    "print(f\"{sec}초는{hour}시간{min}분{re}초입니다.\") # print문으로 출력"
   ]
